<footer class="business-landing-footer" id="<%= data.id %>">
    <div class="business-landing-footer-content">
      <div class="business-landing-footer-logo">
        <img src="<%= footer.image.value %>" class="business-landing-logo" alt="Logo" />
        <nav class="business-landing-footer-nav">
          <ul class="business-header-nav">
            <li><a href="#">Главная</a></li>
            <li><a href="#about">О компании</a></li>
            <li><a href="#services">Услуги</a></li>
            <li><a href="#contact">Контакты</a></li>
          </ul>
        </nav>
        <button
          class="business-landing-button"
          onclick="location.href='<%= footer.buttonLink.value %>'"
        >
          <%= footer.buttonText.value %>
        </button>
      </div>
      <img src="<%= footer.logoImage.value %>" alt="Spark Studio Logo" />
    </div>
</footer>
  