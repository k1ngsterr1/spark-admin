<section class="business-landing-form-content">
  <div class="business-landing-heading-content">
      <%- include('../../components/business-landing-page/mini-text', { text: form.miniText.value, id: form.miniText.id }) %>
      <h4 contenteditable="true" id="<%= form.formHeading.id %>" class="editable business-landing-heading">
          <%= form.formHeading.value %>
      </h4>
      <p contenteditable="true" id="<%= form.formParagraph.id %>" class="editable business-landing-paragraph">
          <%= form.formParagraph.value %>
      </p>
      <form action="" class="business-landing-form">
          <div class="business-landing-inputs">
              <div class="business-inputs-upper">
                  <input type="text" placeholder="<%= form.firstInputPlaceholder.value %>" disabled>
                  <input type="text" placeholder="<%= form.secondInputPlaceholder.value %>" disabled>
              </div>
          </div>
      </form>
  </div>
</section>
